      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>af2b7bc2-6e25-4254-9bc8-158577b66b97</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet_IOPct">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select  top 10 InstanceName as InstName, 
sum (cast(cast(CounterValue as float)/1000000 as decimal(38,2))) as 'SumCounterValue', 
min (cast(cast (CounterValue as float)/1000000 as decimal(38,2))) as 'MinCounterValue', 
avg(cast(cast (CounterValue as float)/1000000 as decimal(38,2))) as 'AvgCounterValue', 
max(cast(cast(CounterValue as float)/1000000 as decimal(38,2)))  as 'MaxCounterValue'
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where dl.objectname in ('Process') and CounterName = 'IO Data Bytes/sec' and InstanceName !='_Total'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime
group by InstanceName
order by 4 desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="instname">
          <DataField>InstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SumCounterValue">
          <DataField>SumCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="MinCounterValue">
          <DataField>MinCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgCounterValue">
          <DataField>AvgCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="MaxCounterValue">
          <DataField>MaxCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_StartTime">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <CommandText>if ( object_id ('counterdata ') is not null)
	select MIN(CounterDateTime) StartTime  from counterdata
else
	select '1990/1/1' as StartTime
	</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_EndTime">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <CommandText>	if ( object_id ('counterdata ') is not null)
	select max(CounterDateTime) EndTime from counterdata
else
	select '2099/1/1' as EndTime</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EndTime">
          <DataField>EndTime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_AvailableTime">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <CommandText>if ( object_id ('counterdata ') is not null)
	Select distinct  CounterDateTime AvailableTime from counterdata order by CounterDateTime
else
	select '1990/1/1' as AvailableTime
	union
	select '2099/1/1' as AvailableTime
	</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="AvailableTime">
          <DataField>AvailableTime</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_CPU">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @proc int
SELECT @proc = Internal_Value
FROM dbo.tbl_XPMSVER
  where name = 'ProcessorCount'

select  top 10 InstanceName as InstName, 
min (cast(CounterValue as decimal(38,2 )))/@proc as 'MinCounterValue', 
avg (cast(CounterValue as decimal(38,2 )))/@proc as 'AvgCounterValue', 
max (cast(CounterValue as decimal(38,2 )))/@proc as 'MaxCounterValue'
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where dl.objectname in ('Process') and CounterName = '% Processor Time' and InstanceName !='_Total'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime
group by InstanceName
order by 4 desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="InstName">
          <DataField>InstName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MinCounterValue">
          <DataField>MinCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgCounterValue">
          <DataField>AvgCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="MaxCounterValue">
          <DataField>MaxCounterValue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_Memory">
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @tbl as table(instname varchar(100) null, minval decimal(38,2), avgval decimal(38,2), maxval decimal(38,2))
insert into @tbl
select  top 9 case when InstanceName = 'sqlservr' then 'SQL Server ( Private Bytes )' else InstanceName end as InstName,
min (cast(CounterValue as decimal(38,2 )))/1000000 as 'MinCounterValue', 
avg (cast(CounterValue as decimal(38,2 )))/1000000 as 'AvgCounterValue', 
max (cast(CounterValue as decimal(38,2 )))/1000000 as 'MaxCounterValue'
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname in ('Process') and CounterName = 'Private Bytes' and InstanceName !='_Total'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime
group by InstanceName
order by 3 desc

insert into @tbl
select  'SQL Server ( Total Server Memory ) ' as InstName,
min (cast(CounterValue as decimal(38,2 )))/1000 as 'MinCounterValue', 
avg (cast(CounterValue as decimal(38,2 )))/1000 as 'AvgCounterValue', 
max (cast(CounterValue as decimal(38,2 )))/1000 as 'MaxCounterValue'
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname like '%Memory Manager' and CounterName = 'Total Server Memory (KB)' 
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime

select instname , minval, avgval, maxval
from @tbl
order by avgval desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="instname">
          <DataField>instname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="minval">
          <DataField>minval</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="avgval">
          <DataField>avgval</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="maxval">
          <DataField>maxval</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
