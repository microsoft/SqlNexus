    <DataSet Name="DataSet_SQLVersion">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID ('dbo.tbl_ServerProperties') IS NOT NULL 
BEGIN 
	SELECT 
	  CASE [PropertyValue] WHEN '9' THEN '2005' 
			WHEN '8' THEN '' 
			WHEN '10' THEN '2008' 
			WHEN '10.50' THEN '2008 R2' 
			WHEN '11' THEN '2012' 
			WHEN '12' THEN '2014' 
			WHEN '13' THEN '2016' 
			WHEN '14' THEN '2017' 
			WHEN '15' THEN '2019' 
			WHEN '16' THEN '2022' 
			else null 
	  END AS 'SQLVersion' 
	FROM 
	  dbo.tbl_ServerProperties 
	WHERE 
	  PropertyName = 'MajorVersion' 
END 
ELSE
	SELECT 'unknown' AS 'SQLVersion'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
