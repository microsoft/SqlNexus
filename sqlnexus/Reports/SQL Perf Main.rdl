    <DataSet Name="DataSet_Import_Report_matched">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID('tblNexusInfo') IS NOT NULL
BEGIN
    DECLARE @ImportVersion NVARCHAR(100),
            @ReportVersion NVARCHAR(100);
    SELECT @ImportVersion = [Value]
    FROM dbo.tblNexusInfo
    WHERE Attribute = 'Nexus Importer Version';
    SELECT @ReportVersion = [Value]
    FROM dbo.tblNexusInfo
    WHERE Attribute = 'Nexus Report Version';
    SELECT @ImportVersion 'ImportVersion',
           @ReportVersion 'ReportVersion',
           CAST(CASE
                    WHEN @ImportVersion = @ReportVersion THEN
                        1
                    ELSE
                        0
                END AS BIT) IsImportReportMatched;
END;
ELSE
BEGIN
    SELECT CAST(NULL AS NVARCHAR(100)) 'ImportVersion',
           CAST(NULL AS NVARCHAR(100)) 'ReportVersion',
           CAST(0 AS BIT) IsImportReportMatched;
END;</CommandText>
      </Query>
      <Fields>
        <Field Name="ImportVersion">
          <DataField>ImportVersion</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ReportVersion">
          <DataField>ReportVersion</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="IsImportReportMatched">
          <DataField>IsImportReportMatched</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_IsLatestDiag">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID('tbl_DiagInfo') IS NOT NULL
BEGIN
    IF EXISTS (SELECT * FROM dbo.tbl_DiagInfo WHERE DiagVersion &gt;= 1001)
        SELECT CAST(1 AS INT) 'IsLatestDiag';
    ELSE
        SELECT CAST(0 AS INT) 'IsLatestDiag';
END;
ELSE
BEGIN
    SELECT CAST(0 AS INT) 'IsLatestDiag';
END;</CommandText>
      </Query>
      <Fields>
        <Field Name="IsLatestDiag">
          <DataField>IsLatestDiag</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
