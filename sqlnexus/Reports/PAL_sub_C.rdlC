  <DataSets>
    <DataSet Name="GetCounters">
      <Query>
        <DataSourceName>dsPerfCounters</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@counters">
            <Value>=Parameters!Counters.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText> declare @sqlCmd nvarchar(max) = 'Select c1.ObjectName  
 ,c1.CounterName  
 ,c1.InstanceName  
 ,CounterDateTime  
 ,c1.CounterValue  
from dbo.Counters c1   
where c1.countername IN (' + (@Counters) + ') or c1.fullcounternamewithwildchar in (' + (@Counters) + ')'  
 exec sp_executesql @sqlcmd  </CommandText>
      </Query>
      <Fields>
        <Field Name="CounterName">
          <DataField>CounterName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CounterDateTime">
          <DataField>CounterDateTime</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CounterValue">
          <DataField>CounterValue</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
