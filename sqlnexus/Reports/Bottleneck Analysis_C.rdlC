    <DataSet Name="DataSet_WaitStats_ParamStartTime">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID('vw_PERF_STATS_SCRIPT_RUNTIMES') IS NOT NULL
BEGIN
    DECLARE @v_StartTime DATETIME;
    DECLARE @v_EndTime DATETIME;
    SELECT @v_StartTime = MIN(runtime)
    FROM dbo.vw_PERF_STATS_SCRIPT_RUNTIMES;
    SELECT @v_EndTime = MAX(runtime)
    FROM dbo.vw_PERF_STATS_SCRIPT_RUNTIMES;
    IF @v_StartTime IS NULL
        SET @v_StartTime = GETDATE();
    IF @v_EndTime IS NULL
        SET @v_EndTime = GETDATE();

    SELECT TOP 1
           StartTime
    FROM
    (
        SELECT CASE
                   WHEN DATEDIFF(mi, @v_StartTime, @v_EndTime) &gt; 4 * 60 THEN
                       DATEADD(mi, -60, @v_EndTime)
                   ELSE
                       @v_StartTime
               END AS StartTime
        UNION ALL
        SELECT @v_StartTime
    ) t
    ORDER BY StartTime ASC;

END;
ELSE
    SELECT CAST('1990/1/1' AS DATETIME) AS StartTime;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_WaitStats_ParamEndTime">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID('vw_PERF_STATS_SCRIPT_RUNTIMES') IS NOT NULL
BEGIN
    DECLARE @v_StartTime DATETIME;
    DECLARE @v_EndTime DATETIME;
    SELECT @v_StartTime = MIN(runtime)
    FROM dbo.vw_PERF_STATS_SCRIPT_RUNTIMES;
    SELECT @v_EndTime = MAX(runtime)
    FROM dbo.vw_PERF_STATS_SCRIPT_RUNTIMES;
    IF @v_StartTime IS NULL
        SET @v_StartTime = GETDATE();
    IF @v_EndTime IS NULL
        SET @v_EndTime = GETDATE();

    SELECT @v_EndTime AS EndTime;


END;
ELSE
    SELECT CAST('1990/1/1' AS DATETIME) AS EndTime;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
