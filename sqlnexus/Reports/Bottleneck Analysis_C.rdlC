  <Code>
    Function GetTableRowFillColor(Theme as String, RowNumber As Integer) As String
    If Theme ="Aquatic" Then
    return "#202020"
    ElseIf Theme ="Desert" Then
    return "#FFFAEF"
    Elseif RowNumber Mod 2 &lt;&gt; 0 Then
    return "White"
    Else
    Return "LightSteelBlue"
    End If
    End Function
  </Code>
