    <DataSet Name="sqlnexus">
      <Fields>
        <Field Name="starting_time">
          <DataField>starting_time</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ending_time">
          <DataField>ending_time</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Duration_Minutes">
          <DataField>Duration_Minutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if (object_id ('tbl_sysprocesses') is not null)
begin
select cast(min(runtime) as char(24)) as starting_time, cast(max(runtime) as char(24)) as ending_time, Datediff(mi, min(runtime), max(runtime)) as Duration_Minutes
from tbl_sysprocesses
end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_HighConcurrency">
      <Fields>
        <Field Name="waitresource">
          <DataField>waitresource</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sum_waittime">
          <DataField>sum waittime</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg_waittime">
          <DataField>avg waittime</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if (object_id ('tbl_sysprocesses') is not null)
begin
select TOP 10 waitresource=convert(varchar(40),waitresource), 'count'=count(*), 'sum waittime'=sum(cast(waittime as bigint)), 'avg waittime'=avg(cast(waittime as bigint))
from dbo.tbl_sysprocesses 
where waitresource &lt;&gt; ' '
group by waitresource having sum(cast(waittime as bigint))  &gt; 0
order by 3 desc
end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_WaittypeSummary">
      <Fields>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sum_waittime">
          <DataField>sum waittime</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg_waittime">
          <DataField>avg waittime</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Type_Description">
          <DataField>Type_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if (object_id ('tbl_sysprocesses') is not null)
begin
select top 7  'count'=count(*), 'sum waittime'=sum(cast(waittime as bigint)), 
'avg waittime'=avg(cast(waittime as bigint)),(case when [lastwaittype] like 'LCK%' then 'Locks' when [lastwaittype] like 'PAGEIO%' then 'Page I/O Latch' when [lastwaittype] like 'PAGELATCH%' then 'Page Latch (non-I/O)' when [lastwaittype] like 'LATCH%' then 'Latch (non-buffer)' when [lastwaittype] like 'IO_COMPLETION' then 'I/O Completion' when [lastwaittype] like 'ASYNC_NETWORK_IO' then 'Network I/O (client fetch)' when [lastwaittype] like 'CLR_%' OR [lastwaittype] like 'SQLCLR%' then 'SQLCLR' when [lastwaittype]='SOS_RESERVEDMEMBLOCKLIST' OR [lastwaittype]='RESOURCE_SEMAPHORE' then 'Memory' when [lastwaittype] like 'RESOURCE_SEMAPHORE_%' OR [lastwaittype]='CMEMTHREAD' then 'Compilation' when [lastwaittype] like 'MSQL_XP' then 'XProc' when [lastwaittype] like 'WRITELOG' then 'Writelog' when [lastwaittype]='CLR_AUTO_EVENT' OR [lastwaittype]='CLR_MANUAL_EVENT' OR [lastwaittype]='SLEEP_TASK' OR [lastwaittype]='DBMIRRORING_CMD' OR [lastwaittype]='DBMIRROR_WORKER_QUEUE' OR [lastwaittype]='CHKPT' OR [lastwaittype]='ONDEMAND_TASK_QUEUE' OR [lastwaittype]='BROKER_TASK_STOP' OR [lastwaittype]='BROKER_RECEIVE_WAITFOR' OR [lastwaittype]='BROKER_EVENTHANDLER' OR [lastwaittype]='BROKER_TRANSMITTER' OR [lastwaittype]='KSOURCE_WAKEUP' OR [lastwaittype]='REQUEST_FOR_DEADLOCK_SEARCH' OR [lastwaittype]='EXCHANGE' OR [lastwaittype]='CXPACKET' OR [lastwaittype]='SQLTRACE_BUFFER_FLUSH' OR [lastwaittype]='LAZYWRITER_SLEEP' OR [lastwaittype]='WAITFOR' or [lastwaittype] = 'CHECKPOINT_QUEUE' or [lastwaittype] = 'LOGMGR_QUEUE' then 'IGNORABLE' else [lastwaittype] end) as Type_Description 
from dbo.tbl_sysprocesses 
where (case when [lastwaittype] like 'LCK%' then 'Locks' when [lastwaittype] like 'PAGEIO%' then 'Page I/O Latch' when [lastwaittype] like 'PAGELATCH%' then 'Page Latch (non-I/O)' when [lastwaittype] like 'LATCH%' then 'Latch (non-buffer)' when [lastwaittype] like 'IO_COMPLETION' then 'I/O Completion' when [lastwaittype] like 'ASYNC_NETWORK_IO' then 'Network I/O (client fetch)' when [lastwaittype] like 'CLR_%' OR [lastwaittype] like 'SQLCLR%' then 'SQLCLR' when [lastwaittype]='SOS_RESERVEDMEMBLOCKLIST' OR [lastwaittype]='RESOURCE_SEMAPHORE' then 'Memory' when [lastwaittype] like 'RESOURCE_SEMAPHORE_%' OR [lastwaittype]='CMEMTHREAD' then 'Compilation' when [lastwaittype] like 'MSQL_XP' then 'XProc' when [lastwaittype] like 'WRITELOG' then 'Writelog' when [lastwaittype]='CLR_AUTO_EVENT' OR [lastwaittype]='CLR_MANUAL_EVENT' OR [lastwaittype]='SLEEP_TASK' OR [lastwaittype]='DBMIRRORING_CMD' OR [lastwaittype]='DBMIRROR_WORKER_QUEUE' OR [lastwaittype]='CHKPT' OR [lastwaittype]='ONDEMAND_TASK_QUEUE' OR [lastwaittype]='BROKER_TASK_STOP' OR [lastwaittype]='BROKER_RECEIVE_WAITFOR' OR [lastwaittype]='BROKER_EVENTHANDLER' OR [lastwaittype]='BROKER_TRANSMITTER' OR [lastwaittype]='KSOURCE_WAKEUP' OR [lastwaittype]='REQUEST_FOR_DEADLOCK_SEARCH' OR [lastwaittype]='EXCHANGE' OR [lastwaittype]='CXPACKET' OR [lastwaittype]='SQLTRACE_BUFFER_FLUSH' OR [lastwaittype]='LAZYWRITER_SLEEP' OR [lastwaittype]='WAITFOR' or [lastwaittype] = 'CHECKPOINT_QUEUE' or [lastwaittype] = 'LOGMGR_QUEUE' then 'IGNORABLE' else [lastwaittype] end) &lt;&gt; 'IGNORABLE'
group by (case when [lastwaittype] like 'LCK%' then 'Locks' when [lastwaittype] like 'PAGEIO%' then 'Page I/O Latch' when [lastwaittype] like 'PAGELATCH%' then 'Page Latch (non-I/O)' when [lastwaittype] like 'LATCH%' then 'Latch (non-buffer)' when [lastwaittype] like 'IO_COMPLETION' then 'I/O Completion' when [lastwaittype] like 'ASYNC_NETWORK_IO' then 'Network I/O (client fetch)' when [lastwaittype] like 'CLR_%' OR [lastwaittype] like 'SQLCLR%' then 'SQLCLR' when [lastwaittype]='SOS_RESERVEDMEMBLOCKLIST' OR [lastwaittype]='RESOURCE_SEMAPHORE' then 'Memory' when [lastwaittype] like 'RESOURCE_SEMAPHORE_%' OR [lastwaittype]='CMEMTHREAD' then 'Compilation' when [lastwaittype] like 'MSQL_XP' then 'XProc' when [lastwaittype] like 'WRITELOG' then 'Writelog' when [lastwaittype]='CLR_AUTO_EVENT' OR [lastwaittype]='CLR_MANUAL_EVENT' OR [lastwaittype]='SLEEP_TASK' OR [lastwaittype]='DBMIRRORING_CMD' OR [lastwaittype]='DBMIRROR_WORKER_QUEUE' OR [lastwaittype]='CHKPT' OR [lastwaittype]='ONDEMAND_TASK_QUEUE' OR [lastwaittype]='BROKER_TASK_STOP' OR [lastwaittype]='BROKER_RECEIVE_WAITFOR' OR [lastwaittype]='BROKER_EVENTHANDLER' OR [lastwaittype]='BROKER_TRANSMITTER' OR [lastwaittype]='KSOURCE_WAKEUP' OR [lastwaittype]='REQUEST_FOR_DEADLOCK_SEARCH' OR [lastwaittype]='EXCHANGE' OR [lastwaittype]='CXPACKET' OR [lastwaittype]='SQLTRACE_BUFFER_FLUSH' OR [lastwaittype]='LAZYWRITER_SLEEP' OR [lastwaittype]='WAITFOR' or [lastwaittype] = 'CHECKPOINT_QUEUE' or [lastwaittype] = 'LOGMGR_QUEUE' then 'IGNORABLE' else [lastwaittype] end)
having sum(cast(waittime as bigint)) &gt; 0
order by 2 desc
end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
