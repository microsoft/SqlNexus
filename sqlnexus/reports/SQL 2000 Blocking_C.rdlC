end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet_Runtimes">
      <Fields>
        <Field Name="rownum">
          <DataField>rownum</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="runtime">
          <DataField>runtime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="blocked_sessions">
          <DataField>blocked_sessions</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="max_waittime">
          <DataField>max_waittime</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if (object_id ('tbl_sysprocesses') is not null)
begin
with cte_runtimes as
(
select rownum,runtime,row_number() over (partition by runtime order by runtime) as rownumber
from tbl_sysprocesses
)
select a.rownum,a.runtime,count(*) as blocked_sessions,max(cast(b.waittime as bigint))/1000 as max_waittime
from cte_runtimes a
left outer join tbl_sysprocesses b
on a.runtime = b.runtime
where a.rownumber  = 1 and b.blocked &lt;&gt; 0
group by a.runtime,a.rownum
having max(cast(b.waittime as bigint)) &gt; 1000
order by a.runtime
end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
